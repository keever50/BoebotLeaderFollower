#include <Arduino.h>
//Test programma follower
void Stepper();
const int Stap = 2;
const int Richting = 5;
void setup()
{
    Serial.begin(9600);
    pinMode(Stap, OUTPUT);
    pinMode(Richting, OUTPUT);
}

void loop()
{
  Stepper();
}

void Stepper()
{
  digitalWrite(Stap, HIGH);
  delay(1);
  digitalWrite(Stap, LOW);
  delay(1);
}